{"ast":null,"code":"import{useState,useEffect}from'react';import axios from'axios';import{BASE_URL}from'../utils/config';const useFetch=(endpoint,queryParams)=>{const[data,setData]=useState([]);const[error,setError]=useState(null);const[loading,setLoading]=useState(false);useEffect(()=>{const fetchData=async()=>{setLoading(true);try{const response=await axios.get(\"\".concat(BASE_URL,\"/\").concat(endpoint),{params:queryParams});setData(response.data.data);}catch(error){setError(error.message);}finally{setLoading(false);}};fetchData();},[endpoint,queryParams]);return{data,error,loading};};export default useFetch;","map":{"version":3,"names":["useState","useEffect","axios","BASE_URL","useFetch","endpoint","queryParams","data","setData","error","setError","loading","setLoading","fetchData","response","get","concat","params","message"],"sources":["/home/soujanya/Music/frontend/src/hooks/useFetch.js"],"sourcesContent":["import { useState, useEffect } from 'react';\nimport axios from 'axios';\nimport { BASE_URL } from '../utils/config';\n\nconst useFetch = (endpoint, queryParams) => {\n  const [data, setData] = useState([]);\n  const [error, setError] = useState(null);\n  const [loading, setLoading] = useState(false);\n\n  useEffect(() => {\n    const fetchData = async () => {\n      setLoading(true);\n\n      try {\n        const response = await axios.get(`${BASE_URL}/${endpoint}`, {\n          params: queryParams,\n        });\n        setData(response.data.data);\n      } catch (error) {\n        setError(error.message);\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    fetchData();\n  }, [endpoint, queryParams]);\n\n  return {\n    data,\n    error,\n    loading,\n  };\n};\n\nexport default useFetch;\n"],"mappings":"AAAA,OAASA,QAAQ,CAAEC,SAAS,KAAQ,OAAO,CAC3C,MAAO,CAAAC,KAAK,KAAM,OAAO,CACzB,OAASC,QAAQ,KAAQ,iBAAiB,CAE1C,KAAM,CAAAC,QAAQ,CAAGA,CAACC,QAAQ,CAAEC,WAAW,GAAK,CAC1C,KAAM,CAACC,IAAI,CAAEC,OAAO,CAAC,CAAGR,QAAQ,CAAC,EAAE,CAAC,CACpC,KAAM,CAACS